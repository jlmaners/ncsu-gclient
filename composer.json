{
    "name": "ncsu/gclient",
    "description": "Package to provide easy facade access to the Google Client API",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.4",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "8.0.4",
        "mockery/mockery": "^1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "NCSU\\GClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NCSU\\GClient\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jason Maners",
            "email": "jlmaners@ncsu.edu"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "NCSU\\GClient\\GoogleClientServiceProvider"
            ],
            "aliases": {
                "GClient": "NCSU\\GClient\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
